#!/usr/bin/env pwsh

# Remove directory ./development
Write-Output "-- Remove directory ./development"
Remove-Item -Recurse -Force ./development

# Clean repository
Write-Output "-- Clean repository"
git reset --hard

# Remove untracked files and directories
Write-Output "-- Remove untracked files and directories"
git clean -fd

# Pull and go to main
Write-Output "-- Pull and go to main"
git pull
git checkout main